form badi_pricing_prepare_tkomk.                            &quot; BvV-01

  data lv_propagate type xfeld.
  data ls_tkomk type komk.

  perform badi_get_handle.

  if gr_badi_mv45afzz is bound.
    lv_propagate = abap_false.
    ls_tkomk = tkomk.
    call badi gr_badi_mv45afzz-&gt;pricing_prepare_tkomk
      exporting
        fxvbap       = xvbap[]
        fxvbpa       = xvbpa[]
        fvbak        = vbak
        ft180        = t180
        fxvbuk       = xvbuk[]
        fxvbkd       = xvbkd[]
        f_t001       = t001
        fvbkd        = vbkd
        fvbap        = vbap
      changing
        cv_propagate = lv_propagate
        ftkomk       = tkomk.
    if lv_propagate = abap_true and ls_tkomk &lt;&gt; tkomk.
      read table tkomk[] transporting no fields with table key
        key_uc = tkomk-key_uc.
      if sy-subrc is initial.
        tkomk-ix_komk = sy-tabix.
        modify tkomk index sy-tabix.
      endif.
    endif.
  endif.

endform.                    &quot;BADI_PRICING_PREPARE_TKOMK

