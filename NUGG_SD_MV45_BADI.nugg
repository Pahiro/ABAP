        f_tvak    = tvak
        fvbap     = vbap
        fvbkd_old = *vbkd
        f_t001    = t001
        fvbak_old = *vbak
        fvbap_old = *vbap
        f_t180    = t180
        f_t683v   = t683v
        f_rv02p   = rv02p
