tables vbrk.
data gr_badi_mv45afzz type ref to zsd_enh_sls_mv45afzz.
data gx_badi_mv45afzz type abap_bool value abap_undefined.

form badi_get_handle.                                       &quot; BvV-01

  if gx_badi_mv45afzz &lt;&gt; abap_undefined
  or gr_badi_mv45afzz is bound.
    return.
  endif.

  gx_badi_mv45afzz = abap_false.

  try.
      get badi gr_badi_mv45afzz.
      gx_badi_mv45afzz = abap_true.
    catch cx_badi.
  endtry.

endform.

form badi_delete_document.                                  &quot; BvV-01

  perform badi_get_handle.

  if gr_badi_mv45afzz is bound.
    call badi gr_badi_mv45afzz-&gt;delete_document
      exporting
        ft180  = t180
      changing
        fvbak  = vbak
        fxvbap = xvbap[]
        fxvbkd = xvbkd[]
        fxvbpa = xvbpa[]
        fxvbep = xvbep[]
        fxvbuk = xvbuk[].
  endif.

endform.                    &quot;BADI_DELETE_DOCUMENT

form badi_move_field_to_vbak.

  perform badi_get_handle.

  if gr_badi_mv45afzz is bound.
    call badi gr_badi_mv45afzz-&gt;move_field_to_vbak
      exporting
        i_tvak  = tvak
        i_tabix = svbak-tabix
        i_xvbap = xvbap[]
        i_xvbpa = xvbpa[]
        i_xvbkd = xvbkd[]
        i_kuagv = kuagv
        i_kuwev = kuwev
      changing
        c_vbak  = vbak
        c_rv45a = rv45a.
  endif.

endform.                    &quot;BADI_MOVE_FIELD_TO_VBAK

form badi_move_field_to_vbap.                               &quot; BvV-01

  perform badi_get_handle.

  if gr_badi_mv45afzz is bound.
    call badi gr_badi_mv45afzz-&gt;move_field_to_vbap
      exporting
        f_tvap    = tvap
        f_tvak    = tvak
        f_tabix   = svbap-tabix
        fvbak     = vbak
        f_maapv   = maapv
        f_kuagv   = kuagv
        fxvbap    = xvbap[]
        fxvbkd    = xvbkd[]
        fvbap_old = *vbap
      changing
        fvbap     = vbap
        ivbap     = ivbap[]
        f_rv45a   = rv45a.
  endif.

endform.                    &quot;BADI_MOVE_FIELD_TO_VBAP

form badi_move_field_to_vbep.                               &quot; BvV-01

