  <method CLSNAME="ZCL_SALV_TABLE" CMPNAME="SET_HOTSPOT" VERSION="1" LANGU="E" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
